::program main

imports :: {
    "florence.org/packages/http"
    "florence.org/packages/log"
}

struct Endpoint {
    string  resourceName
    int     port
}

main :: () {
    http := http.Serve("/route", routeHandler)
}

routeHandler :: (Response resp, Request @req) {
    print("URI is : ${req.Uri}")
    resp.Write("<html>")
}